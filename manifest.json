{
    "manifest_version": 2,

    "name": "GiftMe",
    "description": "This plugin will add a url to your wishlist for you to share.",
    "version": 1.0,
    "browser_action": {
        "default_icon": "icon.png",
        "default_popup": "popup.html"
    },

    "permissions": [
        "activeTab"
    ]
}